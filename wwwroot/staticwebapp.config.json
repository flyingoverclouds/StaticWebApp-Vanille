{
    "routes" : [
        {
            "route": "/secret/*",
            "allowedRoles": ["jaiconfianceenlui"]
        },
        {
            "route": "/superbeimage.jpg",
            "allowedRoles": ["jaiconfianceenlui"]
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        },
        {
            "route": "/api/*",
            "methods": ["PUT", "POST", "PATCH", "DELETE"],
            "allowedRoles": ["administrator"]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*","/secret/*"]
      },
      "responseOverrides": {
        "401": {
          "rewrite": "/error.html"
        }
      }
  }